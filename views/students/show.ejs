        <div class="page-header">
            <h1><a href=""><%= student.firstName + ' ' + student.lastName %></a></h1>
        </div>
        <table class="table">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>School</th>
                    <th>Grade</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><%= student.id %></td>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.school %></td>
                    <td><%= student.grade %></td>
                </tr>
            </tbody>
        </table>
        <hr>
        <a href="/students">Go back</a>
    </div>
<%- include ../partials/footer %>